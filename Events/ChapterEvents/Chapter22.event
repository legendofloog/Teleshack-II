LockeInn:
CHECK_MONEY
IBlt(899, 0xF0) // choice below occurs if gold is above
SetBackground(Tavern)
PreventSkipping
Text(LockeTavernPrompt)
AllowSkipping
SVAL s1 0x1
BEQ 0xAE s1 sC
    Text(LockeNoText) // refuse to recruit
	UnsetFlag(11)
	EraseBackground
	GOTO 0xE

LABEL 0xAE // recruit unit
MUSC(WithUs_Song)
Text(LockeYesText)
CALL $9EE2C4
LOAD1 1 LockeSpawn
ENUN
SVAL s3 900
TAKEMONEY
GOTO 0xE

LABEL 0xF0 // not enough money
Text(LockeNotEnoughText)
// CALL $9EE2C4
UnsetFlag(11)
GOTO 0xE

LABEL 0xE // exit event
NoFade
ENDA

